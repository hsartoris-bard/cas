{
  "dependencies": "hazelcast-ticket-registry,hazelcast-monitor",
  "properties": [
    "--cas.service-registry.core.init-from-json=true",

    "--cas.monitor.endpoints.endpoint.defaults.access=ANONYMOUS",

    "--management.health.hazelcastHealthIndicator.enabled=true",
    "--management.endpoints.web.exposure.include=health",
    "--management.endpoint.health.show-details=always",
    "--management.endpoints.enabled-by-default=true",

    "--cas.ticket.registry.hazelcast.crypto.enabled=true",
    "--cas.ticket.registry.hazelcast.crypto.signing.key-size=512",
    "--cas.ticket.registry.hazelcast.crypto.encryption.key-size=256"
  ]
}
